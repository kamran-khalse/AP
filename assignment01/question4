#include<iostream>
#include<vector>
#include<time.h>
using namespace std;

vector<double> tabe1(vector<double> adad_ha)
{   
    vector<double> adad_ha2(adad_ha.size());
    for(int i=adad_ha.size()-1;i>=0;i--){int j=adad_ha.size()-1-i;
	adad_ha2[j]=adad_ha[i];}
return adad_ha2;};

void tabe2(vector<double> &adad_ha)
{   int x;
    for(int i=0;i<adad_ha.size()/2;i++) {x=adad_ha[i];
    adad_ha[i]=adad_ha[adad_ha.size()-i-1];
    adad_ha[adad_ha.size()-i-1]=x; }  };
	 
	 
	 
int main(){

	vector<double> adad_ha(1000000);    
	
	int t0=time(0);                               //start time
for(int i=0;i<1000;i++){tabe1(adad_ha);}
    int t1=time(0);                                 
for(int i=0;i<1000;i++){tabe2(adad_ha);}    
    int t2=time(0);
    
    cout<<"tabe1 = "<<t1-t0<<"s"<<endl<<"tabe2 = "<<t2-t1<<"s\n";
if((t2-t1)>(t1-t0)){cout<<"tabe1 is faster";}
else if((t2-t1)<(t1-t0)){cout<<"tabe2 is faster";}

return 0;}
